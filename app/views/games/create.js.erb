$("<tr id='<%= @game.name %>'><th><%= escape_javascript(@game.name)%></th><td><%= escape_javascript(@game.genre)%></td><td><%= escape_javascript(@game.publisher)%></td><td><%= escape_javascript(@game.platform)%></td><td><%= escape_javascript(link_to 'Show', @game, :class => 'btn show-button')%></td><td><%= escape_javascript(link_to 'Edit', edit_game_path(@game), :class => 'btn edit-button')%></td><td><%= escape_javascript(link_to 'Delete', @game, :method => 'delete', :class => 'btn del-button', remote: true)%></td></tr>").appendTo('.main-table');